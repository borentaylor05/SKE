<div class="container my formContainer">
	<h1 class="text-center">New User</h1>
	<form>
		<label for="first_name">First Name</label>
		<input class="form-control" name="first_name" type="text" ng-model="user.first_name">
		<label for="last_name">Last Name</label>
		<input class="form-control" name="last_name" type="text" ng-model="user.last_name">
		<label for="email">Email (required, can be fake)</label>
		<input class="form-control" name="email" type="text" ng-model="user.email">
		<label for="client">Client</label>
		<input class="form-control" name="client" type="text" ng-model="user.client">
		<label for="location">Location</label>
		<input class="form-control" name="location" type="text" ng-model="user.location">
		<label for="title">Job Title</label>
		<input class="form-control" name="title" type="text" ng-model="user.title">
		<label for="lob">Line of Business</label>
		<input class="form-control" name="lob" type="text" ng-model="user.lob">
		<div class="text-center">
			<input type="submit" class="btn btn-md btn-success" value="Create User">
		</div>
	</form>
</div>